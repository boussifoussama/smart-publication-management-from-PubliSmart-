     <property name="styleSheet">
      <string notr="true">/* =========================
   TYPOGRAPHIE PREMIUM
   ========================= */
* {
    font-family: &quot;Inter&quot;, &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Helvetica Neue&quot;, Arial, sans-serif;
    font-size: 14px;
    color: #374151;
}

/* =========================
   FENÊTRE PRINCIPALE
   ========================= */
QMainWindow {
    background-color: #F4F6F9;
}

/* =========================
   TITRES &amp; LABELS
   ========================= */
QLabel {
    font-weight: 500;
    color: #1F2937;
}

QGroupBox {
    background-color: #FFFFFF;
    border-radius: 12px;
    border: 1px solid #E5E7EB;
    margin-top: 24px;
    padding: 18px;
    font-weight: 600;
    color: #0F2A44;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 16px;
    font-size: 15px;
    font-weight: 700;
}

/* =========================
   ONGLET (TAB BAR)
   ========================= */
QTabWidget::pane {
    background-color: #FFFFFF;
    border-radius: 12px;
    border: 1px solid #E5E7EB;
    margin-top: 10px;
}

QTabBar::tab {
    background-color: #E5E7EB;
    border-radius: 10px;
    padding: 14px 28px;
    margin-right: 8px;
    font-weight: 500;
    color: #374151;
}

QTabBar::tab:selected {
    background-color: #0F2A44;
    color: #FFFFFF;
    font-weight: 700;
}

QTabBar::tab:hover:!selected {
    background-color: #DBEAFE;
    color: #1E3A8A;
}

/* =========================
   INPUTS (ÉLÉGANTS)
   ========================= */
QLineEdit, QComboBox, QDateEdit {
    background-color: #FFFFFF;
    border-radius: 10px;
    border: 1px solid #D1D5DB;
    padding: 12px;
    font-size: 14px;
    font-weight: 500;
}

QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #2563EB;
    background-color: #F0F9FF;
}

/* =========================
   BOUTONS PREMIUM
   ========================= */
QPushButton {
    background-color: #0F2A44;
    color: #FFFFFF;
    border-radius: 10px;
    padding: 12px 26px;
    font-weight: 700;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    background-color: #163B63;
}

QPushButton:pressed {
    background-color: #2563EB;
}

QPushButton:disabled {
    background-color: #9CA3AF;
    color: #E5E7EB;
