       <string notr="true">/* =========================
   TYPOGRAPHIE PREMIUM
   ========================= */
* {
    font-family: &quot;Inter&quot;, &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Helvetica Neue&quot;, Arial, sans-serif;
    font-size: 14px;
    color: #374151;
}

/* =========================
   FENÊTRE PRINCIPALE
   ========================= */
QMainWindow {
    background-color: #F4F6F9;
}

/* =========================
   TITRES &amp; LABELS
   ========================= */
QLabel {
    font-weight: 500;
    color: #1F2937;
}

QGroupBox {
    background-color: #FFFFFF;
    border-radius: 12px;
    border: 1px solid #E5E7EB;
    margin-top: 24px;
    padding: 18px;
    font-weight: 600;
    color: #0F2A44;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 16px;
    font-size: 15px;
    font-weight: 700;
}

/* =========================
   ONGLET (TAB BAR)
   ========================= */
QTabWidget::pane {
    background-color: #FFFFFF;
    border-radius: 12px;
    border: 1px solid #E5E7EB;
    margin-top: 10px;
}

QTabBar::tab {
    background-color: #E5E7EB;
    border-radius: 10px;
    padding: 14px 28px;
    margin-right: 8px;
    font-weight: 500;
    color: #374151;
}

QTabBar::tab:selected {
    background-color: #0F2A44;
    color: #FFFFFF;
    font-weight: 700;
}

QTabBar::tab:hover:!selected {
    background-color: #DBEAFE;
    color: #1E3A8A;
}

/* =========================
   INPUTS (ÉLÉGANTS)
   ========================= */
QLineEdit, QComboBox, QDateEdit {
    background-color: #FFFFFF;
    border-radius: 10px;
    border: 1px solid #D1D5DB;
    padding: 12px;
    font-size: 14px;
    font-weight: 500;
}

QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border-color: #2563EB;
    background-color: #F0F9FF;
}

/* =========================
   BOUTONS PREMIUM
   ========================= */
QPushButton {
    background-color: #0F2A44;
    color: #FFFFFF;
    border-radius: 10px;
    padding: 12px 26px;
    font-weight: 700;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    background-color: #163B63;
}

QPushButton:pressed {
    background-color: #2563EB;
}

QPushButton:disabled {
    background-color: #9CA3AF;
    color: #E5E7EB;
}

/* =========================
   TABLES (PRO DASHBOARD)
   ========================= */
QTableWidget {
    background-color: #FFFFFF;
    border-radius: 12px;
    border: 1px solid #E5E7EB;
    gridline-color: #E5E7EB;
    alternate-background-color: #F9FAFB;
    selection-background-color: #DBEAFE;
}

QTableWidget::item {
    padding: 14px;
    font-weight: 500;
}

QHeaderView::section {
    background-color: #0F2A44;
    color: #FFFFFF;
    padding: 14px;
    font-weight: 700;
    border: none;
}

/* =========================
   SIDEBAR MODERNE
   ========================= */
#groupBox_11 {
    background-color: #0F2A44;
    border: none;
}

#groupBox_11 QPushButton {
    background-color: transparent;
    color: #FFFFFF;
    padding: 16px 22px;
    border-radius: 12px;
    font-size: 16px;
    font-weight: 600;
}

#groupBox_11 QPushButton:hover {
    background-color: rgba(255,255,255,0.15);
}

#pushButton_13 {
    background-color: rgba(255,255,255,0.25);
    font-weight: 800;
}

/* =========================
   PROGRESS BAR
   ========================= */
QProgressBar {
    background-color: #FFFFFF;
    border-radius: 10px;
    border: 1px solid #D1D5DB;
    text-align: center;
    font-weight: 700;
}

QProgressBar::chunk {
    background-color: #2563EB;
    border-radius: 8px;
}

/* =========================
   MENU &amp; STATUS
   ========================= */
QMenuBar {
    background-color: #0F2A44;
    color: #FFFFFF;
    font-weight: 700;
}

QStatusBar {
    background-color: #F4F6F9;
    color: #6B7280;
}
